<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Payment-Parameters">
    <DisplayName>Extract Payment Parameters</DisplayName>
    <Source>request</Source>
    <URIPath>
        <Pattern ignoreCase="true">/users/{epUserID}/bank-accounts/{epBankID}/payments</Pattern>
    </URIPath>
    <VariablePrefix>urirequest</VariablePrefix>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>