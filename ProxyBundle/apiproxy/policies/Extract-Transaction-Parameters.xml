<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Transaction-Parameters">
    <DisplayName>Extract Transaction Parameters</DisplayName>
    <Source>request</Source>
    <URIPath>
        <Pattern ignoreCase="true">/transactions/{epTransactionID}</Pattern>
    </URIPath>
    <VariablePrefix>urirequest</VariablePrefix>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>